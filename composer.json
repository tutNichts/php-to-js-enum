{
    "authors": [
        {
            "name": "Vincent Petritz",
            "email": "mail@vpetritz.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "TutNichts\\PhpJsEnum\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TutNichts\\PhpJsEnum\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "homepage": "https://github.com/tutNichts/php-to-js-enum",
    "license": "MIT",
    "minimum-stability": "dev",
    "name": "tutnichts/php-to-js-enum",
    "prefer-stable": true,
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^v9.0|^v10.0",
        "illuminate/collections": "^v9.0|^v10.0",
        "symfony/finder": "^6.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.3",
        "phpunit/phpunit": "^9.5"
    },
    "scripts": {
        "analyse": [
            "vendor/bin/phpstan analyse -c phpstan.neon"
        ]
    },
    "type": "library"
}
